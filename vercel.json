{
  "builds": [
    {
      "src": "api/pythonvssunday/pythonvssunday/vercel_app/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/pythonvssunday/pythonvssunday/vercel_app/wsgi.py"
    }
  ]
}